---
interface Props {
  demoName: string;
}
const { demoName } = Astro.props;
const webLink = `https://game-engine-web-examples.vercel.app/?demo=${demoName}`;
---

<div class="relative h-fit w-fit border-2 border-black">
  <iframe src={webLink} class="h-[600px] w-[800px]"> </iframe>
  <a class="absolute right-2 top-2 text-black" href={webLink} target="_blank"
    >full window</a
  >
</div>
